{
  "name": "kurban-cebimde-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "cross-env VITE_PROXY=1 vite",
    "dev:remote": "cross-env VITE_PROXY=0 VITE_API_BASE=https://admin.kurbancebimde.com/api/v1 vite",
    "build": "cross-env VITE_PROXY=0 vite build",
    "build:prod": "tsc && cross-env VITE_PROXY=0 vite build --mode production",
    "preview": "vite preview --port 4173",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:live": "playwright test --grep @live",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "analyze": "vite-bundle-analyzer dist/stats.html"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^5.8.4",
    "@tanstack/react-table": "^8.10.7",
    "agora-rtc-sdk-ng": "^4.24.0",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.1",
    "livekit-client": "^2.15.7",
    "lucide-react": "^0.294.0",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-otp-input": "^3.1.1",
    "react-router-dom": "^6.20.1",
    "react-select": "^5.8.0",
    "recharts": "^2.15.4",
    "serve": "^14.2.5",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "terser": "^5.24.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
