{
  "name": "kurban-cebimde",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:metro": "EXPO_PUBLIC_API_BASE=${EXPO_PUBLIC_API_BASE:-https://admin.kurbancebimde.com} expo start --port 8082 --tunnel",
    "start:dev": "EXPO_PUBLIC_API_BASE=https://admin.kurbancebimde.com expo start --dev-client",
    "start:tunnel": "EXPO_PUBLIC_API_BASE=https://admin.kurbancebimde.com expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "expo r -c",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo-google-fonts/montserrat": "^0.4.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "axios": "^1.11.0",
    "expo": "~54.0.10",
    "expo-audio": "~1.0.13",
    "expo-av": "~16.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.8",
    "expo-device": "~8.0.8",
    "expo-font": "~14.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-localization": "^17.0.7",
    "expo-notifications": "~0.32.11",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "livekit-client": "^2.15.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-agora": "^4.5.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-template-literals": "^7.27.1",
    "@types/react": "~19.1.10",
    "expo-build-properties": "~1.0.9",
    "typescript": "~5.9.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "expo-livekit",
          "react-native-agora",
          "@livekit/react-native-webrtc",
          "react-native-webrtc"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
